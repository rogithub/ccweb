    "description": "web page files",
    "main": "index.js",
    "scripts": {
        "test": "rm -rf ./TypeScript/dist && tsc --build ./TypeScript/tsconfig.json && nyc jasmine --config=jasmine.json",
        "build:webpack": "webpack"
    },
    "nyc": {
        "all": true,
        "exclude": [
            "*.js",
            "TypeScript/raw",
            "TypeScript/pages",
            "TypeScript/spec/",
            "TypeScript/enums/",
	    "TypeScript/constants/",
            "**/[Ii]ndex.ts",
            "wwwroot/",
            "**/*Spec.js"
        ],
        "branches": 80,
        "lines": 80,
        "functions": 80,
        "statements": 80,
        "watermarks": {
            "lines": [
                80,
                95
            ],
            "functions": [
                80,
                95
            ],
            "branches": [
                80,
                95
            ],
            "statements": [
                80,
                95
            ]
        }
    },
