        "@types/bootstrap": "^4.3.1",
        "@types/jasmine": "^3.5.7",
        "@types/jquery": "^3.3.33",
        "@types/knockout": "^3.4.67",
        "jasmine": "^3.5.0",
        "nyc": "^15.0.0",
        "raw-loader": "^4.0.0",
        "ts-loader": "^6.2.1",
        "typescript": "^3.8.2",
        "webpack": "^4.41.6",
        "webpack-cli": "^3.3.11"
